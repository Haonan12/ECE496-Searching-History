{
  "name": "Exam URLs Detector",
  "version": "1.0",
  "description": "Reads your history, and shows the top ten pages you go to by typing the URL.",
  "permissions": [
    "history",
    "alarms",
    "background",
    "tabs",
    "webRequest",
    "webNavigation",
    "storage",
    "http://0.0.0.0:5000/classifyurl"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentscript.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "clock.png"
  },
  "manifest_version": 2
}
